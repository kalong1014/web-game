# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /

#手游
RewriteRule ^(search)/(.*)/([a-zA-Z]+?)\_([0-9]+?).(htm|html|shtml) /index.php\?ac=$1&keyword=$2&orderby=$3&page=$4 [L]
RewriteRule ^(search)/(.*)/ /index.php\?ac=$1&keyword=$2 [L]
RewriteRule ^(down)/(apple|android)/([0-9]+?)_0_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&type=$2&id=$3&d=$4 [L]
RewriteRule ^(down)/(apple|android)/([0-9]+?)_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&type=$2&id=$3&k=$4 [L]
RewriteRule ^(down)/(apple|android)/([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&type=$2&id=$3 [L]

RewriteRule ^(games)/(dateline|score|downloadcount)/([0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&op=$2&yystate=$3&page=$4 [L]
RewriteRule ^(games)/(dateline|score|downloadcount)/([0-9]+?)/ /index.php\?ac=$1&op=$2&yystate=$3 [L]
RewriteRule ^(games)/(dateline|score|downloadcount)/([a-zA-Z0-9]+?)/ /index.php\?ac=$1&cname=$3&op=$2 [L]
RewriteRule ^(games)/(dateline|score|downloadcount)/([a-zA-Z0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&cname=$3&op=$2&page=$4 [L]


RewriteRule ^(games)/(dateline|score|downloadcount)/([a-zA-Z0-9]+?)/([0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&cname=$3&op=$2&yystate=$4&page=$6 [L]
RewriteRule ^(games)/(dateline|score|downloadcount)/([a-zA-Z0-9]+?)/([0-9]+?)/ /index.php\?ac=$1&cname=$3&op=$2&yystate=$4 [L]

RewriteRule ^(games)/([0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&yystate=$2&page=$3 [L]
RewriteRule ^(games)/([0-9]+?)/ /index.php\?ac=$1&yystate=$2 [L]

RewriteRule ^(games)/([a-zA-Z0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&cname=$2&page=$3 [L]
RewriteRule ^(games)/([a-zA-Z0-9]+?)/ /index.php\?ac=$1&cname=$2 [L]
RewriteRule ^(games)/(dateline|score|downloadcount)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&op=$2&page=$3 [L]
RewriteRule ^(games)/(dateline|score|downloadcount)/ /index.php\?ac=$1&op=$2 [L]

RewriteRule ^(app)/([0-9]+?)_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=appdetail&id=$2&page=$3 [L]
RewriteRule ^(app)/([0-9]+?)\.(htm|html|shtml) /index.php\?ac=appdetail&id=$2 [L]
RewriteRule ^(news)/([a-zA-Z0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&cname=$2&page=$3 [L]
RewriteRule ^(news)/([a-zA-Z0-9]+?)/ /index.php\?ac=$1&cname=$2 [L]
RewriteRule ^(news)/([0-9]+?)_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=newdetail&newid=$2&page=$3 [L]
RewriteRule ^(news)/([0-9]+?)\.(htm|html|shtml) /index.php\?ac=newdetail&newid=$2 [L]
RewriteRule ^(packs)/(list)/([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&op=$2&id=$3 [L]

RewriteRule ^(packs)/([0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&pt=$2&page=$3 [L]
RewriteRule ^(packs)/([0-9]+?)/ /index.php\?ac=$1&pt=$2 [L]

RewriteRule ^(testmore|servermore)/([0-9]+?)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&op=$2&page=$3 [L]
RewriteRule ^(testmore|servermore)/([0-9]+?)/ /index.php\?ac=$1&op=$2 [L]

RewriteRule ^(games|recommend|news|packs|test|server)/index_([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&page=$2 [L]
RewriteRule ^(games|recommend|news|packs|test|server)/ /index.php\?ac=$1 [L]
RewriteRule ^(recommend)/(daynum|weeknum|viewnum)/ /index.php\?ac=$1&orderby=$2 [L]
RewriteRule ^(packdetail)/([0-9]+?)\.(htm|html|shtml) /index.php\?ac=$1&packid=$2 [L]

#end 手游

#新加

RewriteRule ^(alllist)/(newsoft|newgame|recomgame|recomsoft)/index_([0-9]+?)\.(htm|html|shtml) /index.php?ac=$1&op=$2&page=$3 [L]
RewriteRule ^(alllist)/(newsoft|newgame|recomgame|recomsoft)/ /index.php?ac=$1&op=$2 [L]
RewriteRule ^(contact)/(copyright|upload|comment|address)\.(htm|html|shtml) /index.php?ac=$1&op=$2 [L]
RewriteRule ^(contact)/ /index.php?ac=$1 [L]
RewriteRule ^(rss)\.(htm|html|shtml) /index.php?ac=$1 [L]
RewriteRule ^(sitemap)_(app|news)/index_([0-9]+?)\.(htm|html|shtml) /index.php?ac=$1&op=$2&page=$3 [L]
RewriteRule ^(sitemap)_(app|news)/ /index.php?ac=$1&op=$2 [L]
RewriteRule ^(mobile)/(.*)\.(htm|html|shtml) /mobile.php\?rewrite=$2 [L]
</IfModule>
# END WordPress